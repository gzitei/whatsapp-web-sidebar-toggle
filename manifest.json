{
  "manifest_version": 3,
  "name": "WhatsApp Web Sidebar Toggle",
  "description": "Hide/show the WhatsApp Web sidebar.",
  "version": "1.0.0",
  "author": "Julian Levy",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
