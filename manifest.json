{
  "manifest_version": 3,
  "name": "Zap Bar",
  "description": "Fix WhatsApp web responsiveness",
  "version": "0.0.3",
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": ["https://web.whatsapp.com/*"],
  "commands": {},
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
